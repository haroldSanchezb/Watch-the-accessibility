<tr class="{ES_PAR}" style="height: 27px; font: normal 12px Arial, sans-serif; color: #333; text-align: right; vertical-align: text-top;">
	{*NORMAL}
	<td style="padding: 0 8px 0 8px; text-align:left;">{_LABEL_DESCUENTO} {DESCUENTO_MONTO} {DESCUENTO_CURRENCY} {NOMBRE_PROMOCION}</td>
	{/NORMAL}
	{*DISCOUNT}
	<td style="padding: 0 8px 0 8px; text-align:left;">{_LABEL_DESCUENTO} {NOMBRE_PROMOCION}</td>
	{/DISCOUNT}
	{*CHILECOMPRA}
	<td style="padding: 0 8px 0 8px;">{_LABEL_DESCUENTO} {_LABEL_CONVENIO_DESCUENTOS_CHILECOMPRA}</td>
	{/CHILECOMPRA}
	<td style="padding: 0 8px 0 8px;">{DESCUENTO_KMS}</td>
	<td style="padding: 0 8px 0 8px;">{DESCUENTO_TARIFA}</td>
	<td style="padding: 0 8px 0 8px;">{DESCUENTO_IMPUESTOS}</td>
	{CAMPO_TUUA}
	{CAMPO_SOBRECARGO}
