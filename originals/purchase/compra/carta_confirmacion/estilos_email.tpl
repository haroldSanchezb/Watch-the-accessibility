h4 {
	font-family:bold 11px Arial, Helvetica, sans-serif;
	background-color: #DCE0EE;
	width: 98%;
	height: 21px;
	border: 1px solid #ccc;
	border-bottom: none;
	margin: 15px auto 0;
	padding: 5px 0 0 0;
	color: #363636;
	text-align: left;
	text-indent: 10px;
}
table {
	width: 600px;
	border: 1px solid #ccc;
	border-bottom: none;
	margin: 0 auto 15px;
	padding: 0;
}
th {
	background-color: #f3f4f9;
	height: 21px;
	border-right: 1px solid #ccc;
	padding: 1px 0 0 5px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: normal 11px;
	color: #003e7e;
	text-align: left;
}
td {
	height: 21px;
	padding: 5px 0 5px 5px;
	background-color: #fff;
	border-right: 1px solid #ccc;
	font-family: Arial, Helvetica, sans-serif;
	font-size: normal 11px;
	color: #333;
	text-align:left;
	vertical-align: text-top;
}







